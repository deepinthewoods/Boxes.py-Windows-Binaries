<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
  <_name>ConcaveKnob</_name>
  <dependency type="executable" location="path">boxes</dependency>
  <id>info.festi.boxes.py.ConcaveKnob</id>
  <param name="generator" type="string" gui-hidden="true">concaveknob</param>
  <param name="inkscape" type="boolean" gui-hidden="true">true</param>
  <param name="tab" type="notebook">

    <page name="0" _gui-text="ConcaveKnob Settings">
      <param name="diameter" type="float" max="9999" _gui-text="diameter" gui-description="Diameter of the knob (mm)">50.0</param>
      <param name="serrations" type="int" max="9999" _gui-text="serrations" gui-description="Number of serrations">3</param>
      <param name="rounded" type="float" max="9999" _gui-text="rounded" gui-description="Amount of circumference used for non convex parts">0.2</param>
      <param name="angle" type="float" max="9999" _gui-text="angle" gui-description="Angle between convex and concave parts">70.0</param>
      <param name="bolthole" type="float" max="9999" _gui-text="bolthole" gui-description="Diameter of the bolt hole (mm)">6.0</param>
      <param name="dhole" type="float" max="9999" _gui-text="dhole" gui-description="D-Flat in fraction of the diameter">1.0</param>
      <param name="hexhead" type="float" max="9999" _gui-text="hexhead" gui-description="Width of the hex bolt head (mm)">10.0</param>
    </page>

    <page name="1" _gui-text="Default Settings">
      <param name="thickness" type="float" max="9999" _gui-text="thickness" gui-description="thickness of the material">3.0</param>
      <param name="tabs" type="float" max="9999" _gui-text="tabs" gui-description="width of tabs holding th parts in place (not supported everywhere)">0.0</param>
      <param name="debug" type="boolean" max="9999" _gui-text="debug" gui-description="print surrounding boxes for some structures">False</param>
      <param name="reference" type="float" max="9999" _gui-text="reference" gui-description="print reference rectangle with given length">100</param>
      <param name="burn" type="float" max="9999" _gui-text="burn" gui-description="burn correction in mm (bigger values for tighter fit)">0.1</param>
    </page>

  </param>
  <effect>
    <object-type>all</object-type>
      <effects-menu>
        <submenu _name="Boxes.py">
         <submenu _name="Parts and Samples"/>
        </submenu>
      </effects-menu>
  </effect>
  <script>
    <command reldir="extensions">boxes.exe</command>
  </script>
</inkscape-extension>
