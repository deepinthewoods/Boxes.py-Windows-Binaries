<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
  <_name>Planetary</_name>
  <dependency type="executable" location="path">boxes</dependency>
  <id>info.festi.boxes.py.Planetary</id>
  <param name="generator" type="string" gui-hidden="true">planetary</param>
  <param name="inkscape" type="boolean" gui-hidden="true">true</param>
  <param name="tab" type="notebook">

    <page name="0" _gui-text="Planetary Settings">
      <param name="sunteeth" type="int" max="9999" _gui-text="sunteeth" gui-description="number of teeth on sun gear">8</param>
      <param name="planetteeth" type="int" max="9999" _gui-text="planetteeth" gui-description="number of teeth on planets">20</param>
      <param name="maxplanets" type="int" max="9999" _gui-text="maxplanets" gui-description="limit the number of planets (0 for as much as fit)">0</param>
      <param name="deltateeth" type="int" max="9999" _gui-text="deltateeth" gui-description="enable secondary ring with given delta to the ring gear">0</param>
      <param name="modulus" type="float" max="9999" _gui-text="modulus" gui-description="modulus of the theeth in mm">3</param>
      <param name="shaft" type="float" max="9999" _gui-text="shaft" gui-description="diameter of the shaft">6.0</param>
    </page>

    <page name="1" _gui-text="Default Settings">
      <param name="thickness" type="float" max="9999" _gui-text="thickness" gui-description="thickness of the material">3.0</param>
      <param name="tabs" type="float" max="9999" _gui-text="tabs" gui-description="width of tabs holding th parts in place (not supported everywhere)">0.0</param>
      <param name="debug" type="boolean" max="9999" _gui-text="debug" gui-description="print surrounding boxes for some structures">False</param>
      <param name="reference" type="float" max="9999" _gui-text="reference" gui-description="print reference rectangle with given length">100</param>
      <param name="burn" type="float" max="9999" _gui-text="burn" gui-description="burn correction in mm (bigger values for tighter fit)">0.1</param>
    </page>

  </param>
  <effect>
    <object-type>all</object-type>
      <effects-menu>
        <submenu _name="Boxes.py">
         <submenu _name="Parts and Samples"/>
        </submenu>
      </effects-menu>
  </effect>
  <script>
    <command reldir="extensions">boxes.exe</command>
  </script>
</inkscape-extension>
