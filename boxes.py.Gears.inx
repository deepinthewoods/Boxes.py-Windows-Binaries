<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
  <_name>Gears</_name>
  <dependency type="executable" location="path">boxes</dependency>
  <id>info.festi.boxes.py.Gears</id>
  <param name="generator" type="string" gui-hidden="true">gears</param>
  <param name="inkscape" type="boolean" gui-hidden="true">true</param>
  <param name="tab" type="notebook">

    <page name="0" _gui-text="Gears Settings">
      <param name="teeth1" type="int" max="9999" _gui-text="teeth1" gui-description="number of teeth">12</param>
      <param name="shaft1" type="float" max="9999" _gui-text="shaft1" gui-description="diameter of the shaft 1">6.0</param>
      <param name="dpercentage1" type="float" max="9999" _gui-text="dpercentage1" gui-description="percent of the D section of shaft 1 (100 for round shaft)">75</param>
      <param name="teeth2" type="int" max="9999" _gui-text="teeth2" gui-description="number of teeth in the other size of gears">32</param>
      <param name="shaft2" type="float" max="9999" _gui-text="shaft2" gui-description="diameter of the shaft2 (zero for same as shaft 1)">0.0</param>
      <param name="dpercentage2" type="float" max="9999" _gui-text="dpercentage2" gui-description="percent of the D section of shaft 1 (0 for same as shaft 1)">0</param>
      <param name="modulus" type="float" max="9999" _gui-text="modulus" gui-description="width of teeth in mm">5</param>
      <param name="pressure_angle" type="float" max="9999" _gui-text="pressure_angle" gui-description="angle of the teeth touching (in degrees)">20</param>
      <param name="profile_shift" type="float" max="9999" _gui-text="profile_shift" gui-description="in precent of the modulus">20</param>
    </page>

    <page name="1" _gui-text="Default Settings">
      <param name="thickness" type="float" max="9999" _gui-text="thickness" gui-description="thickness of the material">3.0</param>
      <param name="tabs" type="float" max="9999" _gui-text="tabs" gui-description="width of tabs holding th parts in place (not supported everywhere)">0.0</param>
      <param name="debug" type="boolean" max="9999" _gui-text="debug" gui-description="print surrounding boxes for some structures">False</param>
      <param name="reference" type="float" max="9999" _gui-text="reference" gui-description="print reference rectangle with given length">100</param>
      <param name="burn" type="float" max="9999" _gui-text="burn" gui-description="burn correction in mm (bigger values for tighter fit)">0.1</param>
    </page>

  </param>
  <effect>
    <object-type>all</object-type>
      <effects-menu>
        <submenu _name="Boxes.py">
         <submenu _name="Parts and Samples"/>
        </submenu>
      </effects-menu>
  </effect>
  <script>
    <command reldir="extensions">boxes.exe</command>
  </script>
</inkscape-extension>
